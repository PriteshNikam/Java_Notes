import java.util.*;

// program to cout a specific Character in String.

public class Main
{
  public static void main(String[] args) {
		String str  = "Maharashtra";
		char c = 'a';
		int count = 0;
		for(int i=0;i<str.length();i++){
		    if(str.charAt(i)==c){
		        count++;
		    }
		}
		System.out.println(count);
	}
}
